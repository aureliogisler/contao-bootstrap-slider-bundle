{
    "name": "xippogmbh/contao-bootstrap-slider-bundle",
    "type": "contao-bundle",
    "description": "Slider Extension which uses bootstrap slider-classes for Contao 4",
    "homepage": "https://www.xippo.ch",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Xippo GmbH",
            "homepage": "https://github.com/xippogmbh"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "^4.4.*",
        "symfony/framework-bundle": "^3.3"
    },
    "conflict": {
        "contao/core": "*",
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "require-dev": {
        "contao/manager-plugin": "^2",
        "doctrine/doctrine-cache-bundle": "^1",
        "friendsofphp/php-cs-fixer": "^2",
        "php-http/guzzle6-adapter": "^1",
        "php-http/message-factory": "^1",
        "phpunit/phpunit": "^5",
        "symfony/phpunit-bridge": "^3"
    },
    "extra": {
        "contao-manager-plugin": "Xippo\\ContaoBootstrapSliderBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "XippoGmbH\\ContaoBootstrapSliderBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XippoGmbH\\ContaoBootstrapSliderBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/xippogmbh/contao-bootstrap-slider-bundle/issues",
        "source": "https://github.com/xippogmbh/contao-bootstrap-slider-bundle"
    }
}
